{
  "private": true,
  "scripts": {
    "clean": "rimraf ./build/*",
    "start": "NODE_ENV=development BABEL_ENV=development babel-node ./server.js",
    "start2":"react-scripts start",
    "build": "npm run clean && react-scripts build",
    "server": "NODE_ENV=production babel-node ./server.js",
    "redeploy": "npm run build && npm run server",
    "postinstall": "linklocal",
    "eject": "react-scripts eject"
  },
  "babel": {
    "presets": [
      "env",
      "react-app"
    ],
    "plugins": [
      "dynamic-import-webpack",
      "transform-ensure-ignore",
      [
        "babel-plugin-transform-require-ignore",
        {
          "extensions": [
            ".css",
            ".sass"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "create-react-server": "^0.3.2",
    "es6-promise": "^4.1.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-ensure-ignore": "^0.1.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "@babel/preset-react": "^7.0.0",
    "babel-register": "^6.26.0",
    "linklocal": "^2.8.1",
    "react-scripts": "^2.1.8",
    "express": "^4.15.5",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^4.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "rimraf": "^2.6.2",
    "webpack-dev-server": "3.1.14"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
